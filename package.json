{
  "name": "mcp-index",
  "version": "0.1.0",
  "description": "MCP Server Index - Crawls, indexes, and serves a directory of MCP servers",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx scripts/seed-database.ts",
    "crawl": "tsx scripts/run-crawler.ts",
    "mcp": "tsx src/mcp/stdio-transport.ts",
    "mcp:score": "tsx scripts/run-scorer.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "index",
    "crawler"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "bullmq": "^5.68.0",
    "cheerio": "^1.2.0",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.7.4",
    "ioredis": "^5.9.2",
    "marked": "^17.0.2",
    "octokit": "^5.0.5",
    "pino": "^10.3.1",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msgpackr-extract"
    ]
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "drizzle-kit": "^0.31.9",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
